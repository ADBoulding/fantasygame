            <string>///Handles the NPC creation
ini_open(working_directory + "\assets\NPCs\" + string_lower(global.location) + "NPC.ini");
workingName = global.localeNPC[| 0];
dialogue = ds_map_create();
if ini_key_exists(workingName, "nameDisc")
{
    dialogue[? "nameDisc"] = ini_read_real(workingName, "nameDisc", workingName);
}

if ini_key_exists(workingName, "intro")
{
    dialogue[? "intro"] = ini_read_real(workingName, "intro", "");
}
if ini_key_exists(workingName, "greet")
{
    dialogue[? "greet"] = ini_read_real(workingName, "greet", "");
}

if ini_key_exists(workingName, "subject")
{
    var subject = ini_read_real(workingName, "subject", "");
    subjects = ds_list_create();
    if (subject != "")
    {
        var arr;
        var del = ",";
        var str = subject + del;
        var len = string_length(del);
        var ind = 0;
        repeat (string_count(del, str)) {
            var pos = string_pos(del, str) - 1;
            arr = string_copy(str, 1, pos);
            str = string_delete(str, 1, pos + len);
            ds_list_add(subjects, arr);
            arr = 0;
        }
    }
    for (i = 0; i &lt; ds_list_size(subjects); i++)
    {
        var sub = subjects[| i];
        if ini_key_exists(workingName, sub+"Visible")
        {
            dialogue[? sub+"Visible"] = ini_read_string(workingName, sub+"Visible", "");
        }
        if ini_key_exists(workingName, sub+"Num")
        {
            for (n = 0; n &lt; ini_read_real(workingName, sub+"Num", -1); n++;)
            {
                dialogue[? sub+string(n)] = ini_read_string(workingName, sub+string(n), "");
                if ini_key_exists(workingName, sub+string(n)+"Progress")
                {
                    dialogue[? (sub+string(n)+"Progress")] = ini_read_string(workingName, sub+string(n)+"Progress", "");
                }
            }  
        }
    }
}
ini_close();
show_message(ds_map_size(dialogue))
show_message(ds_map_size(dialogue))
</string>
